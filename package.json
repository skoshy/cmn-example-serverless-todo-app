{
  "name": "boilerplate-nextjs",
  "version": "0.1.0",
  "repository": "git@github.com:skoshy/boilerplate-nextjs.git",
  "author": "Stefan Koshy <noreply@skoshy.com>",
  "scripts": {
    "help": "env-cmd gulp help",
    "start": "env-cmd gulp start",
    "dev": "env-cmd gulp dev",
    "build": "env-cmd gulp build",
    "httpServe": "env-cmd gulp httpServe",
    "test": "jest src/*",
    "gulp": "env-cmd gulp",
    "lint": "eslint",
    "lint:errors": "eslint \"src/**/*.tsx\" \"src/**/*.ts\" \"src/**/*.jsx\" \"src/**/*.js\" --quiet --no-error-on-unmatched-pattern",
    "prettier": "prettier . --ignore-path .gitignore --write",
    "docker:start": "docker-compose build && docker-compose up",
    "postbuild": "next-on-netlify",
    "postinstall": "gulp postInstall --silent"
  },
  "dependencies": {
    "@supabase/supabase-js": "1.0.5",
    "@types/styled-components": "5.1.4",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "autoprefixer": "10.0.2",
    "classcat": "4.1.0",
    "classnames": "2.2.6",
    "easy-peasy": "4.0.1",
    "grommet": "2.15.2",
    "lodash": "4.17.20",
    "next": "10.0.1",
    "next-on-netlify": "2.6.0",
    "polished": "4.0.3",
    "postcss": "8.1.10",
    "postcss-flexbugs-fixes": "5.0.1",
    "postcss-preset-env": "6.7.0",
    "postinstall-postinstall": "2.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.1.0",
    "react-modal": "3.12.1",
    "sass": "1.29.0",
    "styled-components": "5.2.1",
    "tailwindcss": "2.0.1",
    "twin.macro": "1.12.1",
    "utility-types": "3.10.0",
    "uuid-random": "1.3.2"
  },
  "devDependencies": {
    "@babel/register": "7.12.1",
    "@fullhuman/postcss-purgecss": "3.0.0",
    "@types/lodash": "4.14.165",
    "@types/node": "14.14.7",
    "@types/react": "16.9.56",
    "@types/react-modal": "3.10.6",
    "@typescript-eslint/eslint-plugin": "4.7.0",
    "@typescript-eslint/parser": "4.7.0",
    "babel-plugin-root-import": "6.6.0",
    "babel-plugin-styled-components": "1.11.1",
    "env-cmd": "10.1.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.7",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gulp": "4.0.2",
    "http-serve": "1.0.1",
    "jest": "26.6.3",
    "npm-run": "5.0.1",
    "prettier": "2.1.2",
    "shelljs": "0.8.4",
    "typescript": "4.0.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "styled-components",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  }
}
